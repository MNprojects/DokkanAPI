{
    "endOfLine": "auto",
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.ts",
        "options": {
          "parser": "typescript",
          "singleQuote": true,
          "trailingComma": "all",
          "printWidth": 120,
          "semi": true,
          "tabWidth": 2,
          "useTabs": false,
          "arrowParens": "always",
          "jsxBracketSameLine": false,
          "jsxSingleQuote": true,
          "quoteProps": "consistent"
        }
      }
    ]
  }